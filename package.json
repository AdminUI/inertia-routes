{
	"name": "@adminui/inertia-routes",
	"version": "1.0.0",
	"type": "module",
	"description": "A hybrid JS/PHP package adding Ziggy-routes functionality to your Laravel/Inertia application",
	"main": "./dist/vue/index.cjs",
	"module": "./dist/vue/index.mjs",
	"exports": {
		".": {
			"types": "./dist/vue/types/index.d.ts",
			"import": "./dist/vue/index.mjs",
			"require": "./dist/vue/index.cjs"
		},
		"./vue": {
			"types": "./dist/vue/types/index.d.ts",
			"import": "./dist/vue/index.mjs",
			"require": "./dist/vue/index.cjs"
		},
		"./vue2": {
			"types": "./dist/vue2/types/index.d.ts",
			"import": "./dist/vue2/index.mjs",
			"require": "./dist/vue2/index.cjs"
		}
	},
	"scripts": {
		"build": "pnpm -r build",
		"lint": "npx eslint resources"
	},
	"keywords": [
		"laravel",
		"inertia",
		"ziggy",
		"routes",
		"routing",
		"named routes"
	],
	"author": "Craig Riley <c.riley@evomark.co.uk> (https://evomark.co.uk)",
	"repository": {
		"type": "git",
		"url": "https://github.com/AdminUI/inertia-routes"
	},
	"license": "MIT",
	"dependencies": {
		"@vueuse/core": "^13.3.0",
		"lodash-es": "^4.17.21",
		"qs": "^6.14.0"
	},
	"peerDependencies": {
		"@inertiajs/vue2": "^1.0.0",
		"@inertiajs/vue3": "^2.0.0",
		"react": ">=16.0.0",
		"svelte": ">=3.0.0",
		"vue": ">=3.0.0 || ^2.7.0"
	},
	"peerDependenciesMeta": {
		"vue": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"svelte": {
			"optional": true
		},
		"@inertiajs/vue2": {
			"optional": true
		},
		"@inertiajs/vue3": {
			"optional": true
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.29.0",
		"esbuild": "^0.25.5",
		"esbuild-node-externals": "^1.18.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-vue": "^10.2.0",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vue-eslint-parser": "^10.1.3"
	}
}
