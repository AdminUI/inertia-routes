{
	"name": "@adminui/inertia-routes",
	"version": "1.0.0",
	"type": "module",
	"description": "A hybrid JS/PHP package adding Ziggy-routes functionality to your Laravel/Inertia application",
	"main": "./dist/vue/index.cjs",
	"module": "./dist/vue/index.mjs",
	"types": "./dist/vue/types/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/vue/types/index.d.ts",
			"import": "./dist/vue/index.mjs",
			"require": "./dist/vue/index.cjs"
		},
		"./vue": {
			"types": "./dist/vue/types/index.d.ts",
			"import": "./dist/vue/index.mjs",
			"require": "./dist/vue/index.cjs"
		},
		"./vue2": {
			"types": "./dist/vue2/types/index.d.ts",
			"import": "./dist/vue2/index.mjs",
			"require": "./dist/vue2/index.cjs"
		}
	},
	"scripts": {
		"dev": "pnpm -r build --watch",
		"build": "pnpm -r build",
		"lint": "npx eslint packages",
		"docs:dev": "vitepress dev docs --host",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docs:deploy": "npm run docs:build && gh-pages -d docs/.vitepress/dist"
	},
	"keywords": [
		"laravel",
		"inertia",
		"ziggy",
		"routes",
		"routing",
		"named routes"
	],
	"author": "Craig Riley <c.riley@evomark.co.uk> (https://evomark.co.uk)",
	"repository": {
		"type": "git",
		"url": "https://github.com/AdminUI/inertia-routes"
	},
	"license": "MIT",
	"dependencies": {
		"es-toolkit": "^1.42.0",
		"qs": "^6.14.0"
	},
	"peerDependencies": {
		"@inertiajs/vue2": "^1.0.0",
		"@inertiajs/vue3": "^2.0.0",
		"@vueuse/core": "^11.0.0 || ^12.0.0 || ^13.0.0 || ^14.0.0",
		"react": ">=16.0.0",
		"svelte": ">=3.0.0",
		"vue": ">=3.0.0 || ^2.7.0"
	},
	"peerDependenciesMeta": {
		"vue": {
			"optional": true
		},
		"@vueuse/core": { 
			"optional": true 
		},
		"react": {
			"optional": true
		},
		"svelte": {
			"optional": true
		},
		"@inertiajs/vue2": {
			"optional": true
		},
		"@inertiajs/vue3": {
			"optional": true
		}
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"esbuild": "^0.27.0",
		"esbuild-node-externals": "^1.20.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-vue": "^10.6.2",
		"gh-pages": "^6.3.0",
		"globals": "^16.5.0",
		"prettier": "^3.7.2",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.9.3",
		"vite": "npm:rolldown-vite@^7.2.8",
		"vitepress": "^1.6.4",
		"vue-eslint-parser": "^10.2.0"
	}
}
